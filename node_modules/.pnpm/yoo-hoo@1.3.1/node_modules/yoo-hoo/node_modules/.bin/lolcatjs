#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/j_dev/Documentos/Scolaris/whatsapp/node_modules/.pnpm/lolcatjs@2.4.3/node_modules/lolcatjs/node_modules:/home/j_dev/Documentos/Scolaris/whatsapp/node_modules/.pnpm/lolcatjs@2.4.3/node_modules:/home/j_dev/Documentos/Scolaris/whatsapp/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/j_dev/Documentos/Scolaris/whatsapp/node_modules/.pnpm/lolcatjs@2.4.3/node_modules/lolcatjs/node_modules:/home/j_dev/Documentos/Scolaris/whatsapp/node_modules/.pnpm/lolcatjs@2.4.3/node_modules:/home/j_dev/Documentos/Scolaris/whatsapp/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../lolcatjs@2.4.3/node_modules/lolcatjs/cli.js" "$@"
else
  exec node  "$basedir/../../../../../lolcatjs@2.4.3/node_modules/lolcatjs/cli.js" "$@"
fi
